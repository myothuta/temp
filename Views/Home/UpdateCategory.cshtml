@model SMPWebservice.Models.SearchResponse

@{var searchResponse = Model;}
@if (Model == null)
{
    searchResponse = new SMPWebservice.Models.SearchResponse();
}
else {
    searchResponse = Model;
}


    <div class="right-col-fade-top">
        <div class="module-nav-container">
            <div class="module-nav nav-base" style="border-radius:15px;">
